#include <stdio.h>


int main()
{
    int arr[100],i,key,element;
    printf(" enter no. of elements to be inserted \n");
    scanf("%d",&element);
    printf(" enter elements of the array \n");
    for( i=0;i<element;i++){
    scanf("%d",&arr[i]);}
    
    printf("enter element to search from the array\n");
    scanf("%d", &key);
    
    for(i=0;i<element;i++){
    if(arr[i]==key){
    break;
    }}
    
    if (i<element){
        printf("element location is %d", i+1);
    }
    else{
        printf("element doesnt exist");
    }
    

    return 0;
}
